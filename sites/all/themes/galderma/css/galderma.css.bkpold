.modalDialog{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(36, 33, 33, 0.6);
  z-index: 99999;
  opacity:0;
  -webkit-transition: opacity 400ms ease-in;
  -moz-transition: opacity 400ms ease-in;
  transition: opacity 400ms ease-in;
  pointer-events: none;
  color:#4f4d4d;
}
.modalDialog:target{
    opacity:1;
    pointer-events: auto;
}
.modalDialog > div{
    width: 600px;
    position: relative;
    margin: 25px auto;   
    border-radius: 6px;
  background:#fff;
  border-radius:4px;
  padding-bottom:30px;
}
